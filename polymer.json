{
  "entrypoint": "index.html",
  "shell": "polymer/main-app.html",
  "fragments": [
    "polymer/rank-wrapper.html",
    "polymer/tmdb-movie.html",
    "polymer/tmdb-tv.html",
    "polymer/tweet-wrapper.html",
    "polymer/twitter-friends.html",
    "polymer/social-match.html" 
  ],
  "sources": [
    "polymer/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
  	"preset": "es5-bundled",
  	"addServiceWorker": false,
  	"addPushManifest": false
  }]
}
